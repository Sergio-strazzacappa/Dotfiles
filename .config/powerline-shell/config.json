{
  "segments": [
    "virtual_env",
    "username",
    "time",
    "ssh",
    "cwd",
    "git",
    "hg",
    "jobs",
    "root"
  ],
  "mode": "patched",
  "cwd": {
    "max_depth": 5,
    "max_dir_size": 10,
    "full_cwd": true
  },
  "hostname": {
    "colorize": true
  },
  "git": {
    "show_symbol": true
  },
  "battery": {
    "always_show_percentage": false,
    "low_threshold": 10
  },
  "time": {
    "format": "%H:%M"
  },
  "theme": "~/.config/powerline-shell/themes/my_theme.py"
}
